    <script type="text/javascript">
        function getUrlVars() {
            var vars = {};
            var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {
                vars[key] = value;
            });
            return vars;
        }
        var ug = getUrlVars()["ug"]; //user Guid
        if (ug == undefined) {
            ug = '';
        }
        var is_sim = getUrlVars()["is_sim"]; // this is not needed now.
        if (is_sim == undefined) {
            is_sim = 'f';
        }
        var extid = getUrlVars()["extid"]; //external Member ID
        if (extid == undefined) {
            extid = '';
        }

        var is_t = getUrlVars()["is_t"]; // to show profiles after registrioan.
        if (is_t == undefined) {
            is_t = 's';
        }

        var pg = getUrlVars()["pg"] //user status guid added 06/13/2016
        if (pg == undefined) {
            pg = '';
        }
        var rid = getUrlVars()["rid"] //user status guid added 06/13/2016
        if (rid == undefined) {
            rid = '';
        }
        var sid = getUrlVars()["sid"] //user status guid added 06/13/2016
        if (sid == undefined) {
            sid = '';
        }
        var prjid = getUrlVars()["prjid"] //user status guid added 06/13/2016
        if (prjid == undefined) {
            prjid = '0';
        }
        var into = getUrlVars()["into"] //user status guid added 06/13/2016
        if (into == undefined) {
            into = '';
        }
        if (window.location.href.indexOf("conversant") > -1) {
            //$scope.authkey = '71CC02CD-B8A5-4A5A-AF4D-9988B2F21761';
            $(document).ready(function () {
                $("div#obform").hide();
                $("div#obformbutton").hide();
                $("div#obimg").hide();
            });
        }
        else {
            //$scope.authkey = '9EE17B1A-6882-4CAE-9AED-3C4D4A92DFA9';
            $(document).ready(function () {
                $("div#conversentform").hide();
                $("div#conversentbutton").hide();
                $("div#conversantimg").hide();
            });
        }
        //if (window.location.href) {
        //    $(document).ready(function () {
        //        $("div#conversentform").hide();
        //        $("div#conversentbutton").hide();
        //        $("div#conversantimg").hide();
        //    });
        //}
        //else if (rid == '20907') {
        //    $(document).ready(function () {
        //        $("div#obform").hide();
        //        $("div#obformbutton").hide();
        //        $("div#obimg").hide();
        //    });
        //}

        var pc = getUrlVars()["pc"];//Project cost .
        if (pc == undefined) {
            pc = '';
        }
        else {
            pc = pc.replace(ug, "");
        }


        function takeme() {
            window.location.href = '/cor/ps2?rid=' + rid + '&extid=' + extid + '&prjid=' + prjid + '&sid=' + sid;
        }

      
    </script>




