    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/loading-bar.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/psquestion.css" rel="stylesheet" type="text/css" />
    <style type="text/css">
        .error {
            color: red;
            margin: 7px 0px 0px 4px;
            /* padding-top: 55px; */
            display: inline-block;
        }
    </style>
</head>
<body ng-cloak>
    <div id="dvSurveyQuestions">
        <div class="header" ng-if="isShowOrgLogo">
            <img alt="" id="ctl00_supplierImage" class="header-logo" ng-src="{{orgLogo}}" />
        </div>
        <div class="header" ng-if="cidd == 450">
            <img alt="" id="ctl00_supplierImage" class="header-logo" ng-src="/images/conversant.png" style="width:213px;height:50px;" />
        </div>
        <div class="header" ng-if="cidd == 491">
            <img alt="" id="ctl00_supplierImage" class="header-logo" ng-src="/images/logo_opinionbar2x.png" style="width:213px;height:50px;" />
        </div>
        @RenderBody()
    </div>
    <div class="dvFooter" ng-show="isShowFooter" style="max-width: 800px;font-size:14px;" ng-controller="footerController">
        <div class="pre-footer">
            <div class="copyright-img">
                <div style="font-size: 12px;line-height: 1;">{{powerby}}</div>
                <img src="/images/opinionnetwork_logo.png" alt="" style="width: 150px; padding: 5px;" />
            </div>
            <div class="copyright-block">
                <p>{{footertex}} <a href="#" ng-click="privacyClick()">{{privacy}}</a>  </p>
            </div>
        </div>
    </div>


