<body ng-app="loginApp" ng-controller="loginCtrl">
    <div class="main">
        @Html.AntiForgeryToken()
        <header class="d-flex align-items-center">
            <div class="container">
                <a href="#" class="logo"> <img src="~/images/OB_Logo.png" alt="" height="62px"></a>
            </div>
        </header>
        <section class="mainbody1">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <div class="head-txt-group">
                            <h2>Paying for Netflix is <span>so</span> 2022 </h2>
                            <p>
                                Join Opinionbar today and get your favorite tv or music subscription for FREE when you
                                complete just 5 surveys per month. Choose your favorite, take a few surveys and BOOM,
                                one
                                less bill on your credit card each month!
                            </p>
                            <div class="subscription_logos">
                                <img src="~/images/Logo-Netflix.png" alt="">
                                <img src="~/images/Logo-Apple-Music.png" alt="">
                                <img src="~/images/Logo-AppleTV.png" alt="">
                                <img src="~/images/Spotify-Logo.png" alt="">
                            </div>
                            <div class="subscription_logos">
                                <img src="~/Images/logo_xbox_live_gold.png" alt="" style="max-height: 40px;">
                                <img src="~/Images/logo_EA_Play.svg" alt="" style="max-height: 40px;">
                                <img src="~/Images/logo_ps_plus-removebg-preview.png" alt="" style="max-height: 63px;width: 24%;">
                            </div>
                        </div><br><br><br>
                    </div>
                    <div class="col-md-6">
                        <div class="getstarted">
                            <div class="signup_header">
                                <h6 class="text-white" style="width:inherit;font-size:xx-large">Create Your Account</h6>
                            </div>
                            <div class="signup_body">
                                <div class="signup_form">
                                    <p class="sign-up" style="width:inherit;font-size:x-large">
                                        Sign-up and get started
                                        now!
                                    </p>
                                    <form name="loginForm"
                                          class="form-horizontal register-form"
                                          novalidate="novalidate">

                                        <div>
                                            <input type="text" id="txtFirstName" class="form-control" ng-model="user.FirstName" placeholder="{{'firstName'|translate}}" name="firstName" style="margin-bottom: 6px" ng-change="checkname(user.firstname)" required>
                                            <span ng-show="loginForm.firstName.$invalid && isRegErrMsg" class="error text-danger" style="font-style: italic;">{{'Required'|translate}}</span>
                                            <span ng-if="nameMsg" class="error" style="font-style: italic;">{{'Please enter only letters'|translate}}</span>
                                        </div>

                                        <div>
                                            <div>
                                                <input type="text" id="txtLastName" class="form-control" ng-model="user.LastName" placeholder="{{'lastName'|translate}}" name="lastName" style="margin-bottom: 6px" ng-change="checklname(user.LastName)" required>
                                                <span ng-show="loginForm.lastName.$invalid && isRegErrMsg" class="error text-danger" style="font-style: italic;">{{'Required'|translate}}</span>
                                                <span ng-if="lnameMsg" class="error" style="font-style: italic;">{{'Please enter only letters'|translate}}</span>
                                            </div>
                                        </div>

                                        <div class="padding_mobile">
                                            <input type="email" id="txtEmailAddress1" class="form-control" placeholder="E-mail" ng-model="user.EmailAddress" style="margin-bottom: 6px;" name="email" ng-blur="validateEmail()" required>
                                            <span class="error text-danger" ng-show="loginForm.email.$error.email">{{'notValidEmail'|translate}}</span>
                                            <span ng-show="loginForm.email.$error.required && isRegErrMsg" class="error text-danger" style="font-style: italic;">{{'Required'|translate}}</span>
                                            <span ng-if="emailnotexist" class="error" style="font-style: italic;color:green">{{'emailnotexist'|translate}}</span>
                                            <span ng-if="xVerifyMessageexist" class="error" style="font-style: italic;color:red">{{'emailAddressAlreadyExist'|translate}}</span>
                                            <span ng-if="xVerifyMessage" style="font-style: italic;color:green">{{'emailVerify'|translate}}</span>
                                        </div>

                                        <div>
                                            <input type="text" id="txtAddress1" name="address1" class="form-control" placeholder="{{'address1'|translate}}" style="margin-bottom: 6px;" ng-model="user.Address1" required>
                                            <span class="error text-danger" ng-show="loginForm.address1.$invalid &&isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>


                                        <div>
                                            <input data-val="true" id="address2" name="address2" class="form-control" style="margin-bottom: 6px;" placeholder="{{'address2 - optional'|translate}}" type="text" ng-model="user.Address2">
                                        </div>
                                        <div>
                                            <select class="form-control" id="CountryId" style="margin-bottom: 6px;" ng-model="user.CountryId" name="country" ng-change="countryByStates(user.CountryId)" ng-options="con.CId as con.CN for con in countries" ng-required="true">
                                                <option value="">{{'country'|translate}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="loginForm.country.$invalid && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <!-- ngIf: states.length != 0 -->
                                        <div ng-if="states.length != 0">
                                            <select class="form-control" style="margin-bottom: 6px;" id="ddlStates" ng-model="user.StateId" name="state" ng-options="st.SId as st.SN for st in states" required>
                                                <option value="">{{'stateProvince'|translate}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="loginForm.state.$invalid && isRegErrMsg">{{'Required'|translate}}</span>
                                            <span class="error text-danger" ng-show="loginForm.StateId ==0 && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <div>
                                            <input type="text" id="txtCity" style="margin-bottom: 6px;" class="form-control" name="city" ng-model="user.City" placeholder="{{'city'|translate}}" required />
                                            <span class="error text-danger" ng-show="loginForm.city.$invalid && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <div class="form-group">
                                            <input type="text" id="txtZipCode" style="margin-bottom: 6px;" placeholder="{{'zipPostalCode'|translate}}" class="form-control" ng-model="user.ZipCode" name="zipCode" required>
                                            <span class="error text-danger" ng-show="loginForm.zipCode.$invalid && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>


                                        <div>
                                            <input data-val="true" id="phone" class="form-control" style="margin-bottom: 6px;" name="phone" placeholder="{{'phoneNumber - optional'|translate}}" type="text" maxlength="15" ng-model="user.PhoneNumber">
                                        </div>
                                        <div>
                                            <select id="ddlGender" class="form-control" style="margin-bottom: 6px;" ng-model="user.Gender" name="gender" placeholder="{{'gender'|translate}}" required ng-change="optIntelligenceCheck()">
                                                <option value="">{{'gender'|translate}}</option>
                                                <option value="M">{{'male'|translate}}</option>
                                                <option value="F">{{'female'|translate}}</option>
                                                <option value="N">{{'nonBinary'|translate}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="loginForm.gender.$invalid && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <div>
                                            <select id="ddlMonth" class="form-control" ng-model="user.Month" style="width: 120px;height: 34px;display: inline-block; margin-bottom: 6px;" name="month" required ng-click="optIntelligenceCheck()">
                                                <option value="">{{'month'|translate}}</option>
                                                <option value="1">{{'jan'|translate}}</option>
                                                <option value="2">{{'feb'|translate}}</option>
                                                <option value="3">{{'mar'|translate}}</option>
                                                <option value="4">{{'apr'|translate}}</option>
                                                <option value="5">{{'may'|translate}}</option>
                                                <option value="6">{{'jun'|translate}}</option>
                                                <option value="7">{{'jul'|translate}}</option>
                                                <option value="8">{{'aug'|translate}}</option>
                                                <option value="9">{{'sep'|translate}}</option>
                                                <option value="10">{{'oct'|translate}}</option>
                                                <option value="11">{{'nov'|translate}}</option>
                                                <option value="12">{{'dec'|translate}}</option>
                                            </select>
                                            <select id="ddlDay" class="form-control" ng-model="user.Day" style="width: 80px;height: 34px;margin-top: 2px;display: inline-block; margin-bottom: 6px;" name="day" required ng-click="optIntelligenceCheck()">
                                                <option value="">{{'day'|translate}}</option>
                                                <option value="1">01</option>
                                                <option value="2">02</option>
                                                <option value="3">03</option>
                                                <option value="4">04</option>
                                                <option value="5">05</option>
                                                <option value="6">06</option>
                                                <option value="7">07</option>
                                                <option value="8">08</option>
                                                <option value="9">09</option>
                                                <option value="10">10</option>
                                                <option value="11">11</option>
                                                <option value="12">12</option>
                                                <option value="13">13</option>
                                                <option value="14">14</option>
                                                <option value="15">15</option>
                                                <option value="16">16</option>
                                                <option value="17">17</option>
                                                <option value="18">18</option>
                                                <option value="19">19</option>
                                                <option value="20">20</option>
                                                <option value="21">21</option>
                                                <option value="22">22</option>
                                                <option value="23">23</option>
                                                <option value="24">24</option>
                                                <option value="25">25</option>
                                                <option value="26">26</option>
                                                <option value="27">27</option>
                                                <option value="28">28</option>
                                                <option value="29">29</option>
                                                <option value="30">30</option>
                                                <option value="31">31</option>
                                            </select>
                                            <select class="form-control" id="ddlYear" ng-model="user.Year" name="year" style="width:152px;display: inline-block; margin-bottom: 6px;height: 34px;margin-top: 2px;" required ng-click="optIntelligenceCheck()">

                                                <option value="">{{'year'|translate}}</option>
                                                <option ng-repeat="y in year" value="{{y.key}}">{{y.value}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="(loginForm.month.$invalid ||loginForm.day.$invalid||loginForm.year.$invalid)  && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <!-- ngIf: user.CountryId != 505 && user.CountryId != 512 && user.CountryId != 525 && user.CountryId != 545 && user.CountryId != 548 && user.CountryId != 549 && user.CountryId !=550 && user.CountryId !=559 && user.CountryId !=564 && user.CountryId !=565 && user.CountryId !=572 && user.CountryId !=575 && user.CountryId !=590 && user.CountryId !=596 && user.CountryId !=599 && user.CountryId !=613 && user.CountryId !=618 && user.CountryId !=619 && user.CountryId !=620 && user.CountryId !=628 && user.CountryId != 647 && user.CountryId != 657 && user.CountryId != 668 && user.CountryId != 669 &&
                                        user.CountryId !=673 && user.CountryId !=693 && user.CountryId !=694 && user.CountryId !=700 && user.CountryId !=706 -->
                                        <div ng-if="user.CountryId != 505 && user.CountryId != 512 && user.CountryId != 525 && user.CountryId != 545 && user.CountryId != 548 && user.CountryId != 549 && user.CountryId !=550 && user.CountryId !=559 && user.CountryId !=564 && user.CountryId !=565 && user.CountryId !=572 && user.CountryId !=575 && user.CountryId !=590 && user.CountryId !=596 && user.CountryId !=599 && user.CountryId !=613 && user.CountryId !=618 && user.CountryId !=619 && user.CountryId !=620 && user.CountryId !=628 && user.CountryId != 647 && user.CountryId != 657 && user.CountryId != 668 && user.CountryId != 669 &&
                         user.CountryId !=673 && user.CountryId !=693 && user.CountryId !=694 && user.CountryId !=700 && user.CountryId !=706">
                                            <select class="form-control" style="margin-bottom: 6px;" id="ddlEthnicity" ng-model="user.EthnicityId" name="ethnicity" placeholder="Ethnicity" ng-options="{{'ec.EthnicityId as ec.EthnicityType for ec in ethnicityLst' | translate}}">
                                                <option value="">{{'ethnicity'|translate}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="loginForm.ethnicity.$invalid  && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                            <span class="error text-danger" ng-show="loginForm.EthnicityId ==0 && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <!-- end ngIf: user.CountryId != 505 && user.CountryId != 512 && user.CountryId != 525 && user.CountryId != 545 && user.CountryId != 548 && user.CountryId != 549 && user.CountryId !=550 && user.CountryId !=559 && user.CountryId !=564 && user.CountryId !=565 && user.CountryId !=572 && user.CountryId !=575 && user.CountryId !=590 && user.CountryId !=596 && user.CountryId !=599 && user.CountryId !=613 && user.CountryId !=618 && user.CountryId !=619 && user.CountryId !=620 && user.CountryId !=628 && user.CountryId != 647 && user.CountryId != 657 && user.CountryId != 668 && user.CountryId != 669 &&
                                        user.CountryId !=673 && user.CountryId !=693 && user.CountryId !=694 && user.CountryId !=700 && user.CountryId !=706 -->
                                        <div>
                                            <select class="form-control" style="margin-bottom: 6px;" id="ddlLanguage" ng-model="user.LanguageId" name="planguage" placeholder="Preferred Language" ng-options="ec.LanguageId as ec.LanguageName for ec in langLst" required>
                                                <option value="">{{'preferredLanguage'|translate}}</option>
                                            </select>
                                            <span class="error text-danger" ng-show="loginForm.planguage.$invalid  && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                            <span class="error" ng-show="loginForm.LanguageId ==0 && isRegErrMsg" style="font-style: italic;">{{'Required'|translate}}</span>
                                        </div>
                                        <div class="form-group">
                                            <input type="password" id="txtPassword" style="margin-bottom: 6px;" placeholder="{{'password'|translate}}" class="form-control" ng-model="user.password" name="password" pattern=".{8,}" autocomplete="off" required title={{'eightchar'|translate}}>
                                            <span class="error text-danger" ng-show="loginForm.password.$invalid && isRegErrMsg" style="font-style: italic;">{{'eightchar'|translate}}</span>
                                        </div>
                                        <div class="mb-2">
                                            <select class="form-select" aria-label="Choose Your Reward" ng-model="user.ChooseYourReward" name="ChooseYourReward" placeholder="Choose Your Reward">
                                                <option value="">Choose Your Reward</option>
                                                <option value="1">Netflix</option>
                                                <option value="2">Apple Music</option>
                                                <option value="3">Apple TV+</option>
                                                <option value="4">Spotify</option>
                                                <option value="5">Xbox Live Gold</option>
                                                <option value="6">EA Play</option>
                                                <option value="7">Playstation Plus</option>
                                            </select>
                                        </div>
                                        <div class="form-body">
                                            <div class="flex-container">
                                                <div vc-recaptcha key="'6LfVcForAAAAAHCQH_OWJRVYCXlvU0KlMm2u6B2i'" on-success="setResponse(response)" size="normal"></div>
                                                <label id="lblResult" style="float: left; display: none;color:red;">Invalid Captcha</label>

                                            </div>
                                        </div>
                                        <div class="email2-row" style="display:table; width:100%;">

                                            <div>
                                                <label id="lblResult"
                                                       style="float: left; display: none;color:red;">
                                                    Invalid
                                                    Captcha
                                                </label>
                                                <div class="dvTc" style="padding: 6px;">
                                                    <div class="flex-container1">
                                                        <input style="display: inline-block; height: 10px; padding: 0; vertical-align: top; width: 21px;margin-top: 5px;" name="agree" type="checkbox" value="agre" ng-model="user.agree" required>
                                                        <label style="display: inline-block; line-height: normal; margin: 0; padding-top: 2px; text-align: left; width: 90%; font-size: 12px; cursor: default;" class="ng-binding">
                                                            {{'iagree' | translate}} <a href="" style="color: black;text-decoration: underline;" ng-click="termsClick()" class="ng-binding">{{'termsofuse' | translate}}</a> {{'and' | translate}} {{'additionalSurvays' | translate}}<br />
                                                            {{'privacyGdpr' | translate}} <a href="" style="color: black;text-decoration: underline;" ng-click="privacyClick()" class="ng-binding">{{'privacyPolicy' | translate}} </a>
                                                        </label>
                                                        <span ng-show="loginForm.agree.$invalid && isRegErrMsg" class="error text-danger">{{'Required'|translate}}</span>
                                                    </div>
