@{
    Layout = null;
}
<!DOCTYPE html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>GM Marketplace Insider</title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
    <script src="~/Scripts/angular-cookies.min.js"></script>
    <script src="~/Scripts/angular-translate.min.js" type="text/javascript"></script>
    <script src="~/Scripts/angular-translate-loader-partial.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/services/customServices.js"></script>
    <link href="~/Content/mrktplace/app.css" rel="stylesheet" />
    <link href="~/Content/mrktplace/media.css" rel="stylesheet">
</head>
<body ng-app="loginApp" ng-controller="loginCtrl">
    @Html.AntiForgeryToken()
    <div class="banner_bg">
        <header class="header-top">
            <div class="container">
                <div class="logo-bar space_holder logo-aside">

                    <a href="javascript:;" class="logo-icon"><img src="/Images/General-Motors-logo.png" alt=""></a>
                    <h4 class="logo-title">&nbsp;marketplace insider</h4>
                </div>
                <div class="clear"></div>
            </div>
        </header>
        <section class="content-body">
            <div class="container">
                <div class="insider-row form_cols">
                    <div class="cols aside-head asideCont">
                        <div class="head-group head-group-sm">
                            <h3 class="h3">Marketplace Insider:</h3>
                            <h2 class="h2">Get rewarded!</h2>
                        </div>
                        <div class="get-rewards">
                            <h3 class="title">Get rewarded for every survey you take! </h3>
                        </div>
                        <div class="aside_cards">
                            <div class="cards">
                                <h4 class="title">be an insider!</h4>
                                <div class="card-img">
                                    <p class="card-label" style="padding-top:5px;margin-top:5px;">
                                        Be the first to get a sneak peak
                                        on upcoming GM innovations
                                    </p>
                                    <img src="~/Images/MarketPlaces/img_insider.png" alt="" style="margin-top:30px;">
                                </div>
                            </div>
                            <div class="cards">
                                <h4 class="title">be heard!</h4>
                                <div class="card-img">
                                    <p class="card-label" style="padding-top:5px;margin-top:5px;">
                                        Your input will shape
                                        GM products of the future
                                    </p>
                                    <img src="~/Images/MarketPlaces/img_heard.png" alt="" style="margin-top:45px;">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="cols reg_form subscriber">
                        <div class="login-form">
                            <form novalidate name="userInput">
                                <div class="reg_form_head">
                                    <div class="btn-signup"> Finish Signing Up</div>
                                    <p>
                                        <b>You have completed the qualification survey and you are in!</b><br />
                                        <br/>
                                        <br/>
                                        Thank you for your participation.<br />
                                        Please complete the registration from below to finalize your enrollment. Be on the lookout for your next invitation to participate in a Marketplace Insider survey. We will send them periodically to the email address you have provided. We look forward to hearing from you soon!
                                    </p>
                                </div>
                                <div class="forminline">
                                    <label class="form-label">First Name</label>
                                    <input type="text" id="txtFirstName" class="form-control" name="firstName" ng-model="user.FirstName"
                                           placeholder="FirstName" ng-required="true" />
                                    <span ng-show="userInput.firstName.$invalid && isRegErrMsg" class="text-danger">{{'required'|translate}}</span>
                                </div>
                                <div class="forminline">
                                    <label class="form-label">Last Name</label>
                                    <input type="text" id="txtLastName" class="form-control" name="lastName" ng-model="user.LastName"
                                           placeholder="LastName" required />
                                    <span ng-show="userInput.lastName.$invalid && isRegErrMsg" class="text-danger">{{'required'|translate}}</span>
                                </div>

                                <div class="forminline">
                                    <label class="form-label">Email Address</label>
                                    <input type="email" class="survey-form-control" name="emailId" id="emailId" ng-model="user.EmailAddress" ng-blur="validateEmail()" placeholder="Email Address" ng-required="true">
                                    <span ng-show="userInput.emailId.$error.required && isRegErrMsg" class="text-danger">{{'required' | translate}}</span>
                                    <span class="error text-danger txt-req" ng-show="userInput.emailId.$error.email">{{'notValidEmail'|translate}}</span>
                                    <span ng-show="emailXVerify==1 ||emailXVerify==0 " ng-class="{'text-danger':emailXVerify==0,'text-success':emailXVerify==1}">{{xVerifyMessage}}</span>
                                </div>
                                <div class="forminline">
                                    <label class="form-label">Password</label>
                                    <input type="password" id="txtPassword" class="form-control" name="password" ng-model="user.Password" placeholder="Password" required />
                                    <span class="error text-danger" ng-show="userInput.password.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                    <span class="text-danger" ng-show="step2Form.password.$error.minlength">Minimum of 6 Characters</span>
                                </div>

                                @*<div class="forminline">
                                    <label class="form-label">Address1 </label>
                                    <input type="text" id="txtAddress1" class="form-control" name="address1" ng-model="user.Address1"
                                           placeholder="Address1" ng-required="true" />
                                    <span class="error text-danger" ng-show="userInput.address1.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                </div>*@
                                @*<div class="forminline">
                                    <label class="form-label">Address2 </label>
                                    <input type="text" id="txtAddress2" class="form-control" data-bind="value: Address2"
                                           placeholder="Address2" />
                                </div>*@

                                <div class="forminline">
                                    <label class="form-label">Country </label>
                                    <select class="form-control txt-box" id="CountryId" ng-model="user.CountryId" name="country" ng-change="countryByStates(user.CountryId)" ng-options="con.CountryId as con.CountryName for con in countries" ng-required="true">
                                        <option value="">{{'Country'|translate}}</option>
                                    </select>
                                    <span class="error text-danger" ng-show="userInput.country.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                    <span class="error text-danger" ng-show="user.CountryId ==0 && isRegErrMsg">{{'required'|translate}}</span>
                                </div>
                                @*<div class="forminline">
                                    <label class="form-label">State/Province </label>
                                    <select class="form-control txt-box" id="ddlStates" ng-model="user.StateId" name="state" ng-options="st.StateId as st.StateName for st in states" required>
                                        <option value="">{{'State'|translate}}</option>
                                    </select>
                                    <span class="error text-danger" ng-show="userInput.state.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                    <span class="error text-danger" ng-show="user.StateId ==0 && isRegErrMsg">{{'required'|translate}}</span>
                                </div>*@

                                @*<div class="forminline">
                                    <label class="form-label">City </label>
                                    <input type="text" id="txtCity" class="form-control" name="city" ng-model="user.City" placeholder="City" required />
                                    <span class="error text-danger" ng-show="userInput.city.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                </div>*@
                                <div class="forminline">
                                    <label class="form-label">Zip/Postal Code </label>
                                    <input type="text" id="txtZipCode" class="form-control" name="zipCode" ng-model="user.ZipCode" placeholder="Zip Code" required />
                                    <span class="error text-danger" ng-show="userInput.zipCode.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                </div>

                                @*<div class="forminline">
                                    <label class="form-label">Mobile </label>
                                    <input type="text" id="txtPhoneNumber" class="form-control" ng-model="user.PhoneNumber"
                                           placeholder="Phone Number" />
                                </div>*@
                                <div class="forminline">
                                    <label class="form-label">Gender </label>
                                    <select id="ddlGender" class="form-control" ng-model="user.Gender" name="gender" placeholder="Gender" required ng-change="optIntelligenceCheck()">
                                        <option value="">{{'gender'|translate}}</option>
                                        <option value="m">Male</option>
                                        <option value="f">Female</option>
                                        <option value="N">Non-Binary</option>
                                    </select>
                                    <span class="text-danger " ng-show="userInput.gender.$invalid && isRegErrMsg">{{'required'|translate}}</span>
                                </div>
                                @*<div class="forminline">
                                    <label class="form-label">Ethnicity </label>
                                    <select class="form-control" id="ddlEthnicity" ng-model="user.EthnicityId" name="ethnicity" placeholder="Ethnicity" ng-options="ec.EthnicityId as ec.EthnicityType for ec in ethnicityLst" required>
                                        <option value="">{{'ethnicity'|translate}}</option>
                                    </select>
                                    <span class="error text-danger " ng-show="userInput.ethnicity.$invalid  && isRegErrMsg">{{'required'|translate}}</span>
                                    <span class="error text-danger" ng-show="user.EthnicityId ==0 && isRegErrMsg">{{'required'|translate}}</span>
                                </div>*@
                                <div class="forminline" style="width:100%;">
                                    <label class="form-label">Date of Birth </label>
                                    <div class="row_col">
                                        <div class="cols-3">
                                            <select id="ddlMonth" class="form-control" ng-model="user.Month" style="width: 100px;
                                            display: inline-block;" name="month" required ng-click="optIntelligenceCheck()">
                                                <option value="">MM</option>
                                                <option value="01">January</option>
                                                <option value="02">February</option>
                                                <option value="03">March</option>
                                                <option value="04">April</option>
                                                <option value="05">May</option>
                                                <option value="06">June</option>
                                                <option value="07">July</option>
                                                <option value="08">August</option>
                                                <option value="09">September</option>
                                                <option value="10">October</option>
                                                <option value="11">November</option>
                                                <option value="12">December</option>
                                            </select>
                                        </div>
                                        <div class="cols-3">
                                            <select id="ddlDay" class="form-control" ng-model="user.Day" style="width: 95px;
                                            display: inline-block;" name="day" required ng-click="optIntelligenceCheck()">
                                                <option value="">DD</option>
                                                <option value="01">01</option>
                                                <option value="02">02</option>
                                                <option value="03">03</option>
                                                <option value="04">04</option>
                                                <option value="05">05</option>
                                                <option value="06">06</option>
                                                <option value="07">07</option>
                                                <option value="08">08</option>
                                                <option value="09">09</option>
                                                <option value="10">10</option>
                                                <option value="11">11</option>
                                                <option value="12">12</option>
                                                <option value="13">13</option>
                                                <option value="14">14</option>
                                                <option value="15">15</option>
                                                <option value="16">16</option>
                                                <option value="17">17</option>
                                                <option value="18">18</option>
                                                <option value="19">19</option>
                                                <option value="20">20</option>
                                                <option value="21">21</option>
                                                <option value="22">22</option>
                                                <option value="23">23</option>
                                                <option value="24">24</option>
                                                <option value="25">25</option>
                                                <option value="26">26</option>
                                                <option value="27">27</option>
                                                <option value="28">28</option>
                                                <option value="29">29</option>
                                                <option value="30">30</option>
                                                <option value="31">31</option>
                                            </select>
                                        </div>
                                        <div class="cols-3">
                                            <select class="form-control" id="ddlYear" ng-model="user.Year" name="year" ng-options="y.key as y.value for y in year" convert-to-number style="width: 95px;display: inline-block;" required ng-click="optIntelligenceCheck()">
                                                <option value="">YY</option>
                                            </select>

                                        </div>
                                        <span class="error text-danger " ng-show="(userInput.month.$invalid ||userInput.day.$invalid||userInput.year.$invalid)  && isRegErrMsg">{{'required'|translate}}</span>
                                    </div>
                                </div>
                                
                                <div class="forminline" style="width:88%;">
                                    <div class="col-md-10 pull-right">
                                        <div vc-recaptcha key="'6LetblorAAAAAMH3OWFI3rtURCyE4SGpXp9gkfk1'" on-success="setResponse(response)" size="normal"></div>
                                        <span class="text-danger" ng-show="captchaErr">Invalid captcha selection.</span>
                                    </div>

                                </div>
                                <div id="divShow" class="form-group" style="display:none;">
                                    <div style="color: Black; margin-left: 10px; padding-left: 10px; text-align:justify;">

                                        <b>{{'goodnews'|translate}}! </b>{{'basenonDemographics'| translate}}.<br /><br />
                                        <div id="offers" oi-pid="14376727" class="form-group" style="vertical-align:left;">

                                        </div>

                                    </div>
                                </div>
                                <div class="chk_term">
                                    <div class="dvTc">
                                        @*<input style="display: inline-block; height: 10px; padding: 0; vertical-align: top; width: 10px;margin-top: 8px;" name="agree" type="checkbox" value="agre" ng-model="user.agree" required>*@
                                        <label style="display: inline-block; line-height: normal; margin: 0; padding-top: 2px; text-align: left; width: 89%; font-size: 12px; cursor: default;" class="ng-binding">
                                            By signing up, I agree to the <a href="" style="color: white;text-decoration: underline;" ng-click="termsClick()" class="ng-binding">Terms of Use</a> and <a href="" style="color: white;text-decoration: underline;" ng-click="privacyClick()" class="ng-binding">Privacy Policy </a>and agree to receive additional survey, market research and other relevant offers from Marketplace-insider.com
                                        </label>
                                        @*<span ng-show="userInput.agree.$invalid && isRegErrMsg" class="text-danger">{{'required'|translate}}</span>*@
                                    </div>

                                    <div class="btn_submit">
                                        <input type="submit" id="imgNext" data-bind="value: ViewModel1.BodyHeaders.Submit" name="subscribe" ng-click="submit(userInput.$valid)" value="Submit" class="button-newsletter">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="footer-links"><a href="" ng-click="termsClick('/WidgetTerms.html')">Terms and Conditions</a>  |  <a href="" ng-click="privacyClick()">Privacy Policy</a> | <a href="#" ng-click="doNotSellMyInfo()">Do Not Sell My Info</a></div> 
            </div>
        </section>
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/angular-recaptcha/4.1.1/angular-recaptcha.min.js"> </script>
    @Scripts.Render("~/bundles/bootstrap")
    <script type="text/javascript" src="~/Scripts/pages/login.js"></script>
</body>

