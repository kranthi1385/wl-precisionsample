                                <div class="optiontext" data-bind="foreach:ViewModel.Questions()[$parentContext.$parentContext.$index()].SpecialOptinLst[$index()]">
                                    <div style="width: 15%;" class="grid_column" data-bind="attr: {id:QuestionId+'_'+OptionId+'_dvmain'}">
                                        <!--ko if: OptionText == 'Prefer not to answer' || OptionText == 'None of the above' -->
                                        <div class="grid_radio" data-bind="attr: {id:QuestionId+'_'+OptionId+'_radio'},event: {click:chkwithradioclick.bind($root,$parent)}">
                                        </div>
                                        <div class="grid_span_holder">
                                            <span class="grid_span" data-bind="text:QuesitonText,attr: {id:QuestionId+'_'+OptionId+'_rdDiv'},event: {click:chkwithradioclick.bind($root,$parent)}"></span>
                                        </div>
                                        <input type="radio" style="display: none;" data-bind="attr: {id:OptionId +'_radio',value:OptionId, name: QuestionId+'_rd','class':QuestionId+'_selectrdb'}, checked:SpeicalOptionId" />
                                        <!-- /ko -->
                                        <!--ko if: OptionText != 'Prefer not to answer' && OptionText != 'None of the above' -->
                                        <div class="grid_matrix_checkbox" data-bind="attr: {id:QuestionId+'_'+OptionId+'_checkbox',name: QuestionId+'_checkbox'},event: {click:chkwithradioclick.bind($root,$parent)}">
                                        </div>
                                        <div class="grid_span_holder">
                                            <span class="grid_span" data-bind="text:QuesitonText,attr: {id:QuestionId+'_'+OptionId+'_chkDiv'},event: {click:chkwithradioclick.bind($root,$parent)}"></span>
                                        </div>
                                        <input type="checkbox" style="display: none;" data-bind="attr: { id:OptionId+'_checkbox',value:OptionId,name: QuestionId+'_checkbox'}, checked:IsChecked " />
                                        <!-- /ko -->
                                    </div>

                                </div>

                            </div>
                        </div>
                        <div>
                            <span class="error" align="left" style="color: Red; display: none; float: left; width: 100%; font-size: 13px; text-align: left;" data-bind="attr:{ id: $parentContext.$parentContext.$index()+'_'+$index()+'_spn'}" />
                        </div>
                    </div>

                    <!-- /ko -->

                </div>
            </div>
            <div class="question_martix" style="min-height: 30px;">
                <div class="validationspqs">
                    <div style="width: 38%;" class="grid_question">
                    </div>
                    <div data-bind="foreach:ChildQuestionList" style="font-size: 14px;">
                        <div style="width: 16%;" class="grid_column">
                            <span class="error" align="left" style="color: Red; float: left; width: 100%; font-size: 13px; text-align: left; display: none" data-bind="attr:{ id: QuestionId+'_spn'}" />
                        </div>
                    </div>
                </div>

            </div>
        </script>

        <script type="text/x-jquery-tmpl" id="ChildRadioTemplate">
            <td>
                <table>
                    <tr>
                        <td style="padding-top: 5px; padding-bottom: 5px;">
