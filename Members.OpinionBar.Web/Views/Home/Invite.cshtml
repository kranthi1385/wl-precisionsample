@{
    Layout = "~/Views/Shared/_bannertab.cshtml";

}
<link href="~/Content/tafResponsive.css" rel="stylesheet" />


<style>
    .down-table table {
    float:none !important;
    display:table;
}

.row_downline{
    padding: 20px;
    max-width: 1060px;
    margin: auto;
}

.btn {
    display: inline-block;
    padding: 6px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
}
.btn-default {
    color: #333;
    background-color: #fff;
    border-color: #ccc;
}
</style>

<div class="down-table row_downline">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
    }
    @*<input id="antiForgeryToken" ng-model="antiForgeryToken" ng-init="antiForgeryToken='@GetAntiForgeryToken()'" />*@
    <ng-include src="getPartialView">
    </ng-include>
</div>
