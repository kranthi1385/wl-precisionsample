      <setting name="Members_PrecisionSample_Prescreener_com_imperium_ra1_raService" serializeAs="String">
        <value>http://ra.imperium.com/raservice.asmx</value>
      </setting>
    </Members.PrecisionSample.Prescreener.Properties.Settings>
  </applicationSettings>
</configuration>